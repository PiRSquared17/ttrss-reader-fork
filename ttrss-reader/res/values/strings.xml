    <string name="CacheImagesOnStartupTitle">Start ImageCache on startup?</string>
    <string name="CacheImagesOnStartupSummary">Enable to start the image-cache on every start of the reader, all articles+images will be fetched in the background.</string>
    <string name="CacheImagesOnlyWifiTitle">Only on Wifi?</string>
    <string name="CacheImagesOnlyWifiSummary">Sets the automatic ImageCache on startup to only run when Wifi is used.</string>
    <string name="DisplayAlignFlushLeftTitle">Left-aligned Text</string>
    <string name="DisplayAlignFlushLeftSummary">Article text will be aligned flush left, ragged right. Default is justified.</string>
    <string name="DisplayDateTimeFormatSystemTitle">Date/Time format</string>
    <string name="DisplayDateTimeFormatSystemSummary">Enable to use the system\'s default for displaying date and time. Disable to use a custom format.</string>
    <string name="DisplayDateFormatTitle">Custom date format</string>
    <string name="DisplayDateFormatSummary">Specify a DateFormat string for the date (see Wiki). Defaults to \"dd.MM.yyyy\". Invalid values will be silently ignored and the default will be used.</string>
    <string name="DisplayDateFormatDefault">dd.MM.yyyy</string>
    <string name="DisplayTimeFormatTitle">Custom time format</string>
    <string name="DisplayTimeFormatSummary">Specify a DateFormat string for the time (see Wiki). Defaults to \"kk:mm\". Invalid values will be silently ignored and the default will be used.</string>
    <string name="DisplayTimeFormatDefault">kk:mm</string>
    <string name="DisplayDarkBackgroundTitle">Use dark background?</string>
    <string name="DisplayDarkBackgroundSummary">Enable this to display the articles with a dark background for easier reading at night.</string>
